<!--can't wait-->

<category>
  <pattern>CANTWAIT</pattern>
  <template>Well, you can always try another library. Morley is especially lovely at this time of year. 
 </template>
</category>

<category>
  <pattern>_ can not wait</pattern>
  <template><srai>CANTWAIT</srai></template>
</category>

<category>
  <pattern>_ can not wait *</pattern>
  <template><srai>CANTWAIT</srai></template>
</category>

<category>
  <pattern>can not wait *</pattern>
  <template><srai>CANTWAIT</srai></template>
</category>


